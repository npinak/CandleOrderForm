          "helpGroup": "global",
          "allowNo": false,
          "aliases": [
            "pc"
          ]
        },
        "appId": {
          "name": "appId",
          "type": "option",
          "char": "i",
          "description": "The id of the app to manage environment variables for",
          "multiple": false,
          "aliases": [
            "a"
          ]
        },
        "mode": {
          "name": "mode",
          "type": "option",
          "char": "m",
          "description": "management mode",
          "multiple": false,
          "options": [
            "list-keys",
            "set",
            "delete"
          ]
        },
        "key": {
          "name": "key",
          "type": "option",
          "char": "k",
          "description": "variable key [required for set and delete]]",
          "multiple": false,
          "relationships": [
            {
              "type": "all",
              "flags": [
                {
                  "name": "mode"
                }
              ]
            }
          ]
        },
        "value": {
          "name": "value",
          "type": "option",
          "char": "v",
          "description": "variable value [required for set]",
          "multiple": false,
          "relationships": [
            {
              "type": "all",
              "flags": [
                {
                  "name": "mode"
                }
              ]
            }
          ]
        }
      },
      "args": {}
    },
    "code:logs": {
      "id": "code:logs",
      "description": "Stream logs",
      "strict": true,
