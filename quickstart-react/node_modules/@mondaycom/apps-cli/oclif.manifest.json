    "code:env": {
      "id": "code:env",
      "description": "Manage environment variables for your app hosted on monday-code.",
      "strict": true,
      "pluginName": "@mondaycom/apps-cli",
      "pluginAlias": "@mondaycom/apps-cli",
      "pluginType": "core",
      "aliases": [],
      "examples": [
        "<%= config.bin %> <%= command.id %>"
      ],
      "flags": {
        "verbose": {
          "name": "verbose",
          "type": "boolean",
          "description": "Print advanced logs (optional).",
          "helpGroup": "global",
          "allowNo": false
        },
        "print-command": {
          "name": "print-command",
          "type": "boolean",
